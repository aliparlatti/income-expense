{
  "name": "ionic-angular-tailwindcss-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "ng build",
    "fmt": "prettier --write . && ng lint --fix",
    "lint": "ng lint",
    "ng:update-angular-cli": "ng update @angular/cli --force && prettier --write package.json",
    "ng:update-angular-core": "ng update @angular/core --force && prettier --write package.json",
    "ng:update-angular-eslint": "ng update @angular-eslint/schematics --force && prettier --write package.json",
    "start": "ng serve"
  },
  "dependencies": {
    "@angular/common": "^18.0.1",
    "@angular/compiler": "^18.0.1",
    "@angular/core": "^18.0.1",
    "@angular/forms": "^18.0.1",
    "@angular/platform-browser": "^18.0.1",
    "@angular/platform-browser-dynamic": "^18.0.1",
    "@angular/router": "^18.0.1",
    "@ionic/angular": "^8.2.0",
    "ionicons": "^7.4.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^18.0.1",
    "@angular-eslint/eslint-plugin": "^18.0.1",
    "@angular-eslint/eslint-plugin-template": "^18.0.1",
    "@angular-eslint/schematics": "^18.0.1",
    "@angular-eslint/template-parser": "^18.0.1",
    "@angular/build": "^18.0.2",
    "@angular/cli": "^18.0.2",
    "@angular/compiler-cli": "^18.0.1",
    "@angular/language-service": "^18.0.1",
    "@types/node": "^20.12.13",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "eslint-plugin-tailwindcss": "^3.17.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.5.0",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.5"
  },
  "engines": {
    "node": ">=20.13.1",
    "pnpm": ">=9"
  }
}
